{
  "name": "connect-qos",
  "version": "4.1.0",
  "description": "Connect middleware that helps maintain a high quality of service during heavy traffic",
  "main": "lib",
  "types": "src",
  "scripts": {
    "bench": "node --experimental-fetch ./tools/bench.js",
    "build": "npx rimraf ./lib && swc src --out-dir lib",
    "prepublish": "npm run build",
    "test": "jest",
    "watch": "swc src --out-dir lib -w"
  },
  "files": [
    "lib",
    "src",
    "docs",
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/node-connect-qos"
  },
  "keywords": [
    "connect",
    "qos",
    "quality",
    "service",
    "layer7",
    "ddos"
  ],
  "author": {
    "name": "Aaron Silvas",
    "email": "asilvas@godaddy.com"
  },
  "license": "MIT",
  "dependencies": {
    "lru-cache": "^7.14.0",
    "toobusy-js": "^0.5.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.249",
    "@swc/jest": "0.2.22",
    "@tsconfig/node16": "1.0.3",
    "@types/jest": "^28.1.8",
    "@types/node": "18.7.11",
    "chokidar": "3.5.3",
    "jest": "^28.1.3",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.2.229"
  }
}
