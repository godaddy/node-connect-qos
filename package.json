{
  "name": "connect-qos",
  "version": "5.5.3",
  "description": "Connect middleware that helps maintain a high quality of service during heavy traffic",
  "main": "lib",
  "types": "src",
  "scripts": {
    "bench": "node --experimental-fetch --cpu-prof --cpu-prof-dir './cpu-profiles/' ./tools/bench.js",
    "build": "npx rimraf ./lib && swc src/* --strip-leading-paths --out-dir lib",
    "prepublish": "npm run build",
    "test": "jest",
    "watch": "swc src --out-dir lib -w"
  },
  "files": [
    "lib",
    "src",
    "docs",
    "package.json",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/node-connect-qos"
  },
  "keywords": [
    "connect",
    "qos",
    "quality",
    "service",
    "layer7",
    "ddos"
  ],
  "author": {
    "name": "Aaron Silvas",
    "email": "asilvas@godaddy.com"
  },
  "license": "MIT",
  "dependencies": {
    "lru-cache": "^7.14.1",
    "toobusy-js": "^0.5.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "@swc/jest": "^0.2.36",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "chokidar": "3.5.3",
    "jest": "^29.7.0",
    "rimraf": "3.0.2",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.2.229"
  }
}
